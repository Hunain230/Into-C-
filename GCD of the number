#include <iostream>
using namespace std;

int main() {
    int a, b;

   
    cout << "Enter two integers: ";
    cin >> a >> b;

 
    if (a < 0) a = -a;
    if (b < 0) b = -b;

   
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }

   
    cout << "The GCD is: " << a << endl;
