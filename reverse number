#include <iostream>
using namespace std ;

int main() {
    int number = 12345;
    int reversedNumber = 0;
    while (number > 0) {
        int digit = number % 10; 
        reversedNumber = reversedNumber * 10 + digit; 
        number /= 10; 
    }
    cout << "Reversed number: " << reversedNumber << endl;

    return 0;
}
